{
  "name": "openseadragon-filters",
  "version": "2.0.0",
  "description": "OpenSeadragon plugin providing filtering capabilities to the images.",
  "type": "module",
  "main": "dist/cjs/openseadragon-filtering.js",
  "module": "dist/esm/openseadragon-filtering.js",
  "types": "dist/types/openseadragon-filtering.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/openseadragon-filtering.js",
      "require": "./dist/cjs/openseadragon-filtering.js"
    }
  },
  "engines": {
    "node": ">=20.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:dual",
    "build:dual": "npm run clean && tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json",
    "watch": "tsc --watch",
    "clean": "node ./scripts/clean.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rssaini01/openseadragon-filters.git"
  },
  "keywords": [
    "openseadragon",
    "filters",
    "image",
    "osd"
  ],
  "author": "Ravi Shankar Saini",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rssaini01/openseadragon-filters/issues"
  },
  "homepage": "https://github.com/rssaini01/openseadragon-filters#readme",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/openseadragon": "^5.0.1",
    "openseadragon": "^5.0.1",
    "typescript": "^5.9.3"
  }
}
